class Position {
    x = y = 0
    def move(nx,ny) {
        x = nx; y = ny
    }
}
class Pos3D extends Position {
    z = 0
    def set(nx,ny,nz) {
        x = nx;y = ny;z = nz
    }
}
class Factory {
    def instance() {
        p = Pos3D.new
    }
}

p = Factory.new.instance()
p.move(3,4)
print p.x + p.y
p.set(5,6,7)
p.z = 8
print p.x + p.y + p.z